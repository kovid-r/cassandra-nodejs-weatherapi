{
  "name": "cassandra-nodejs-weatherapi",
  "version": "1.0.0",
  "description": "This GitHub repository uses Node.js to query the WeatherAPI.com API to fetch cryptocurrency prices and send them to Cassandra. The data from Cassandra can be used by any downstream application for tracking cryptocurrency prices and trading them.",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/kovid-r/cassandra-nodejs-weatherapi.git"
  },
  "scripts": {
    "lint": "eslint lib",
    "start": "node index.js --unhandled-rejections=none --log-level=error"
  },
  "author": "Kovid Rathee",
  "bugs": {
    "email": "kovid.rathee@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3"
  },
  "dependencies": {
    "async": "^1.5.2",
    "aysnc": "0.0.1-security",
    "cassandra-driver": "^4.6.3",
    "config": "^3.3.6",
    "moment": "^2.17.1",
    "request": "^2.55.0"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "commonjs": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "no-underscore-dangle": 0,
      "strict": 0,
      "prefer-rest-params": 0
    }
  }
}
